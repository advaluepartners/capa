{
  "version": 2,
  "builds": [
    {
      "src": "apps/studio/package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NPM_FLAGS": "--legacy-peer-deps --no-audit --no-fund"
  },
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps --no-audit --no-fund"
    }
  },
  "installCommand": "npm install $NPM_FLAGS && npm install -g node-gyp@latest && npm install --save-dev typescript",
  "buildCommand": "cd apps/studio && npm run build"
}



  